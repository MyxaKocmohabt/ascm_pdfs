<script language="javascript" type="text/javascript">

 	var leftminutes;
 	var minutes;
 	
 	var hiddentextboxname ="uctrlFullHeader_ShutdownAlert1_Hidden1";
 	var hiddentextboxname2 ="uctrlFullHeader_ShutdownAlert1_Hidden2";
 	var maindev ="uctrlFullHeader_ShutdownAlert1_divNews";
 	var maindev1 ="uctrlFullHeader_ShutdownAlert1_mainDivShutdown";
 	
function blinkOn()
{

	if(document.getElementById(maindev1))
	{
	    var pagerequest = document.location.href;
	    
		if (pagerequest.toUpperCase().indexOf("LOGON") > 0)
		{
		
			document.getElementById(maindev1).className = "shutdownmsg"
				var table = document.getElementById ("shutdowntable");
						if (table.rows.length > 1) {
							var x=table.rows[0].cells;
							if(x.length>1)
							{
								x[1].innerText="";
							}
						}
						
		}
		else if(pagerequest.toUpperCase().indexOf("ASPX") <= 0)
		{
			document.getElementById(maindev1).className = "shutdownmsg"
				var table = document.getElementById ("shutdowntable");
						if (table.rows.length > 1) {
							var x=table.rows[0].cells;
							if(x.length>1)
							{
								x[1].innerText="";
							}
						}
						
		}
		else if(pagerequest.toUpperCase().indexOf("MAP")>0)
		{
						document.getElementById(maindev1).className = "shutdownmsgalternateformap";
						var table = document.getElementById ("shutdowntable");
						if (table.rows.length > 1) {
							table.deleteRow(0);
							var x=table.rows[0].cells;
							if(x.length>0)
							{
								x[0].colSpan="1";
							}
						}
						
		}
		else 
		{
			document.getElementById(maindev1).className = "shutdownmsgalternate";
		}
	}
	
	if (document.getElementById(hiddentextboxname)) 
	{
		minutes =document.getElementById(hiddentextboxname).value; 
		if(minutes > 0)
		{
			if(document.getElementById("divBlink"))
			{	
				if(document.getElementById("divBlinkFinal"))
				{				 
					if (document.getElementById("divBlinkFinal").innerText != "")
					{
						document.getElementById(hiddentextboxname2).value = document.getElementById("divBlinkFinal").innerText;
						var divParent = document.getElementById(maindev);
						var divChild = document.getElementById("divBlinkFinal");
						divParent.removeChild(divChild);
					}
				}
				var message = document.getElementById("divBlink").innerHTML.replace("[X]",minutes);
				message = message.replace(leftminutes,minutes);
				if (minutes==1)
				{	
					message = message.replace("minutes","minute");
				}				
				
				document.getElementById(maindev).className="";
				
				document.getElementById("divBlink").innerHTML=message;
				document.getElementById("divBlink").style.visibility="visible";
				leftminutes = minutes;
				document.getElementById(hiddentextboxname).value = minutes-1;
				setTimeout("blinkOff()",57000);
				return;
			}
		}
		else
		{
			if((document.getElementById("divBlinkFinal") == null))
			{
				if(document.getElementById(maindev))
				{
					var newdiv = document.createElement("div");
					newdiv.id = "divBlinkFinal";
					newdiv.innerText = document.getElementById(hiddentextboxname2).value ;
					var divParent = document.getElementById(maindev);
					divParent.appendChild(newdiv);
				}
			}
			if(document.getElementById("divBlink"))
			{
				var divParent = document.getElementById(maindev);
				var divChild = document.getElementById("divBlink");
				divParent.removeChild(divChild);
			}
			if(document.getElementById("divBlinkFinal"))
			{
				document.getElementById("divBlinkFinal").style.visibility="visible";				
			}
			setTimeout("blinkOff()",1500);
			return;
			
		}
	}
    setTimeout("blinkOff()",100);
    
}
 
function blinkOff()
{
	if (document.getElementById(hiddentextboxname)) 
	{
		minutes =document.getElementById(hiddentextboxname).value; 
		if(minutes > 0)
		{
			if(document.getElementById("divBlink"))
				{
					document.getElementById("divBlink").style.visibility="hidden";
				}
		}
		else
		{
			if(document.getElementById("divBlinkFinal"))
			{
				document.getElementById("divBlinkFinal").style.visibility="hidden";
			}
		}	
		setTimeout("blinkOn()",100);
   }
}
function jschange() {

	if(document.getElementById(maindev1).style.display=="none") {
		document.getElementById(maindev1).style.display="block";
} else {
	document.getElementById(maindev1).style.display="none";
	}
}
